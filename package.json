{
  "name": "react-uniformed",
  "version": "0.0.7",
  "description": "Scalable forms for React using hooks",
  "main": "dist/react-uniformed.js",
  "author": "Jay Harris <jay@sourware.com>",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/j-a-y-h/react-uniformed.git",
  "homepage": "https://github.com/j-a-y-h/react-uniformed",
  "keywords": [
    "react",
    "hooks",
    "form",
    "uniform",
    "validation",
    "react-hooks",
    "react-hooks-form",
    "react-form",
    "react-form-hooks"
  ],
  "scripts": {
    "test": "jest --silent",
    "test:coverage": "rimraf ./coverage && yarn test --coverage --silent || true",
    "test:coveralls": "yarn test --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint src/**/*.ts",
    "size": "yarn bundle:prod --silent && bundlesize",
    "clean": "rimraf ./dist",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "open:report": "open-cli ./coverage/report.html -- 'google chrome'",
    "open:coverage": "open-cli ./coverage/lcov-report/index.html -- 'google chrome'",
    "bundle:prod": "yarn build --config ./webpack.prod.config.js",
    "build": "yarn clean && webpack",
    "build:prod": "yarn lint && yarn test && yarn bundle:prod",
    "build:js": "yarn clean && yarn tsc ./src/index.ts --target ESNEXT --outdir dist &> /dev/null",
    "build:docs:api": "yarn build:js || jsdoc2md ./dist/use*.js > ./docs/API.md"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.13.0",
    "bundlesize": "^0.18.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "2.18.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "jest-html-reporters": "^1.1.8",
    "jsdoc-to-markdown": "^5.0.0",
    "open-cli": "^5.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.9.0",
    "react-hooks-testing-library": "^0.5.1",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "bundlesize": [
    {
      "path": "./dist/react-uniformed.min.js",
      "maxSize": "4 kB"
    }
  ]
}
